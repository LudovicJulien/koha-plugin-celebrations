{
  "feux-artifice": {
    "font_url": "https://fonts.googleapis.com/css2?family=Orbitron:wght@600&display=swap",
    "elements": {
      "couleurs": {
        "setting": "couleur_feux",
        "file": "feux-artifice-color",
        "type": "css"
      },
      "feux": {
        "setting": "animation_feux",
        "file": "feux-artifice-animation",
        "type": "both"
      }
    }
  },
  "halloween": {
    "font_url": "https://fonts.googleapis.com/css2?family=Creepster&display=swap",
    "elements": {
      "couleurs": {
        "setting": "couleur_halloween",
        "file": "halloween-color",
        "type": "css"
      },
      "footer": {
        "setting": "footer_halloween",
        "file": "halloween-footer",
        "type": "both"
      },
      "ghost": {
        "setting": "activation_ghost",
        "file": "halloween-ghost",
        "type": "both"
      },
      "spider": {
        "setting": "activation_spiders",
        "file": "halloween-spider",
        "type": "both",
        "extra_options": {
          "quantite_spiders": {
            "type": "range",
            "min": 1,
            "max": 6,
            "default": 6
          }
        }
      }
    }
  },
  "noel": {
    "font_url": "https://fonts.googleapis.com/css2?family=Mountains+of+Christmas&display=swap",
    "elements": {
      "couleurs": {
      "setting": "couleur_noel",
      "file": "noel-color",
       "type": "css"
      },
      "footer": {
      "setting": "footer_noel",
      "file": "noel-footer",
      "type": "both"
      },
      "snow": {
        "setting": "activation_flocons",
        "file": "noel-flocon",
        "type": "both",
        "extra_options": {
          "vitesse_flocons": {
              "type": "select",
              "option_type": "option_vitesse"
          },
          "taille_flocons": {
              "type": "select",
              "option_type": "option_taille"
          },
          "vent_flocons": {
              "type": "select",
              "option_type": "option_vent"
          },
          "quantite_flocons": {
              "type": "range",
              "min": 5,
              "max": 200,
              "default": 100
          }
        }
      },
      "countdown": {
      "setting": "countdown_noel",
      "file": "noel-countdown",
      "type": "both"
      }
    }
  },
  "paque": {
    "font_url": "https://fonts.googleapis.com/css2?family=Chewy&display=swap",
    "elements": {
      "couleurs": {
        "setting": "couleur_paque",
        "file": "paque-color",
         "type": "css"
      },
      "footer": {
        "setting": "footer_paque",
        "file": "paque-footer",
        "type": "both"
      },
      "egg": {
        "setting": "activation_eggs",
        "file": "paque-egg",
        "type": "js",
        "extra_options": {
          "api_namespace": {
            "type": "ignore"
          }
        }
      }
    }
  },
  "saint-valentin": {
    "font_url": "https://fonts.googleapis.com/css2?family=Eater&display=swap",
    "elements": {
      "couleurs": {
        "setting": "couleur_valentin",
        "file": "valentin-color",
         "type": "css"
      },
      "coeur": {
        "setting": "activation_coeurs",
        "file": "valentin-coeur",
        "type": "both",
        "extra_options": {
          "vitesse_coeurs": {
              "type": "select",
              "option_type": "option_vitesse"
          },
          "taille_coeurs": {
              "type": "select",
              "option_type": "option_taille"
          },
          "vent_coeurs": {
              "type": "select",
              "option_type": "option_vent"
          },
          "quantite_coeurs": {
              "type": "range",
              "min": 5,
              "max": 200,
              "default": 50
          }
        }
      }
    }
  }
}
